            <stringProp name="parameters"></stringProp>
            <stringProp name="script">String cookieHeader = prev.getResponseHeaders();
def matcher = cookieHeader =~ /csrftoken=([^;]+)/;
if (matcher.find()) {
    vars.put(&quot;auth_cookie&quot;, matcher.group(1));
} else {
    log.error(&quot;Session ID &lt;csrftoken&gt; cookie not found!&quot;);
}
props.put(&quot;auth_cookie&quot;, vars.get(&quot;auth_cookie&quot;));
props.put(&quot;csrf_token_admin&quot;, vars.get(&quot;csrf_token_admin&quot;));

</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
